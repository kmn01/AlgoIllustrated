<% var page='stacks_interaction' ; %>
<%- include('../common/head') %>
<body>

	<%- include('../common/head', {page: page}) %>
	<%- include('../common/topbar', {page: page}) %>
	<%- include('../common/leftbar', {page: page}) %>
	<%- include('../common/rightbar', {page: page}) %>
	<%- include('../common/bottombar', {page: page}) %>

	<div id="popup_instr">
		<h2>Instructions</h2>
		<h4  id="instructions_content"></h4>
		<p id="search" class="execAction" onclick=closeInstr()>Close</p>
	</div>

	<div id="graphsvg"></div>
	<div id="popup_out">
		<h2 style="color: aliceblue;">Output</h2>
		<p id="status_out"></p>
		<p id="search" class="execAction" onclick="location.href='/stacks_simulation';">Close</p>
	</div>
	<div id="output_div" style="position: absolute; top: 580px; left: 350px; width: fit-content; background-color: rgb(143, 217, 217);"><p id="output" style="font-size:x-large; font-family: 'Courier New', Courier, monospace;"></p></div>
	<div id="output_div" style="position: absolute; top: 600px; left: 350px; width: fit-content;"><p id="output_hi" style="font-size:x-large; font-family: 'Courier New', Courier, monospace; color: red;"></p></div>
	<button id="push_btn" style="left: 800px;" onclick=pushalg()>Push</button>
	<input id="pushed" style="position: absolute; top: 100px; left: 870px; visibility: hidden; font-size: x-large; width: 45px;">
	<button id="pop_btn" style="left: 940px;" onclick=popalg()>Pop</button>
	<button id="rev_btn" style="left: 1000px;" onclick=reversed()>Reversed</button>
	<button id="bal_btn" style="left: 1000px;" onclick=balanced()>Balanced</button>
	<button id="unbal_btn" style="left: 1115px;" onclick=unbalanced()>Unbalanced</button>
	
	<script src="code_expl.js"></script>
	<script src="stack_data.js"></script>
	<script src="stack_functions.js"></script>
	<script src="stack_linkedlist.js"></script>
	<script src="stack.js"></script>

</body>